{
  "plugins": ["prettier-plugin-svelte"],
  "singleQuote": true,
  "overrides": [
    {
      "files": ["**/*.css", "**/*.html", "**/*.yml"],
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": ["**/*.svelte"],
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
